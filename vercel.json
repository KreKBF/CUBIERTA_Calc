{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.cubierta.org https://*.wix.com https://*.wixsite.com;"
        }
      ]
    }
  ]
}
